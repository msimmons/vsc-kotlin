{
  "name": "vsc-kotlin",
  "displayName": "VSC Kotlin",
  "description": "Kotlin Language Support (jvmcode Integration)",
  "version": "1.0.0",
  "publisher": "contrapt",
  "engines": {
    "vscode": "^1.42.0"
  },
  "icon": "resources/kotlin.png",
  "repository": {
    "url": "https://github.com/msimmons/vsc-kotlin"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:*.kt"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "views": {},
    "commands": [],
    "menus": {},
    "configuration": {
      "title": "Kotlin Configuration"
    },
    "problemMatchers": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile; ./gradlew build deploy",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "mocha -r ts-node/register test/unit/**/*.test.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.10.59",
    "@types/vscode": "^1.42.0",
    "@types/parsimmon": "^1.10.1",
    "chai": "^4.2.0",
    "mocha": "^3.5.0",
    "ts-node": "^8.8.1",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "parsimmon": "^1.13.0"
  },
  "extensionDependencies": [
    "contrapt.jvmcode"
  ]
}
